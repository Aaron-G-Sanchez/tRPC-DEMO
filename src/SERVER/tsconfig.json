{
 "compilerOptions": {
  "target": "es6",
  "module": "es6",
  "rootDir": "../",
  "moduleResolution": "node",
  "resolveJsonModule": true,
  "inlineSourceMap": true,
  "outDir": "../../dist",
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "strict": true,
  "skipLibCheck": true
 } ,
 "exclude": ["node_modules"],
 "include": [
  "*.ts", "users.json"  
 ]
}